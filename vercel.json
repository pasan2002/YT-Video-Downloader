{
  "version": 2,
  "buildCommand": "cd ytdownloader && npm install && npm run build",
  "outputDirectory": "ytdownloader/build",
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "/video-download",
      "dest": "/server/server.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "server/server.js": {
      "runtime": "nodejs18.x"
    }
  }
}

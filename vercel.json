{
  "version": 2,
  "builds": [
    {
      "src": "ytdownloader/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "ytdownloader/build"
      }
    },
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "/video-download",
      "dest": "/server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/ytdownloader/$1"
    }
  ]
}
